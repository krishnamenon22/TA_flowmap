  "scripts": {
    "dev:tailwind": "tailwindcss -i src/styles/tailwind.css -o src/styles/index.css -w",
    "dev:react": "env-cmd -f ./.env react-scripts start",
    "start": "npm-run-all --parallel dev:*",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f ./.env.development react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --env=jsdom --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "typescript": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint src --color",
    "prettier": "prettier src --check",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint-staged": "lint-staged --config lint-staged.js",
    "husky-install": "cd .. && husky install web/.husky",
    "start-auth": "node server.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@storybook/addon-essentials": "7.4.6",
    "@storybook/addon-interactions": "7.4.6",
    "@storybook/addon-links": "7.4.6",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.4.6",
    "@storybook/preset-create-react-app": "7.4.6",
    "@storybook/react": "7.4.6",
    "@storybook/react-webpack5": "7.4.6",
    "@storybook/testing-library": "0.2.2",
    "@types/babel__core": "^7.20.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.5.6",
    "@types/plotly.js": "^2.12.18",
    "@types/react-plotly.js": "^2.6.0",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "babel-plugin-named-exports-order": "0.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "json-server": "^0.17.2",
    "jsonwebtoken": "^9.0.0",
    "lint-staged": "^13.1.1",
    "multer": "^1.4.5-lts.1",
    "prettier": "^2.8.3",
    "prop-types": "15.8.1",
    "storybook": "7.4.6",
    "tailwindcss": "^3.2.6",
    "ts-jest": "^29.1.1",
    "tsc-files": "^1.1.3",
    "webpack": "5.88.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/stories/*",
      "!src/index.tsx",
      "!src/client.ts",
      "!src/urls.ts",
      "!src/authConfig.ts"
    ]
  }
}
